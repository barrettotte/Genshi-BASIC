expression -> literal
            | unary
            | binary
            | grouping ;

literal    -> NUMERIC | STRING ;
grouping   -> "(" expression ")" ;
unary      -> ( "-" | "NOT" ) expression ;
binary     -> expression operator expression ;
operator   -> "=" | "-" | "+" | "/" | "*" | "^" | ">" | "<"
            | "<=" | "=>" | ">=" | "=>" | "<>"